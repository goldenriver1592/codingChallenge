{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "lib": [
      "ES2020",
      "dom"
    ],
    "moduleResolution": "bundler",
    "sourceMap": true,
    "types": [
      "node",
      "cypress",
      "allure-cypress",
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.d.ts",
    "cypress.config.ts"
  ]
}